{
  "name": "@qwertykeith/claude-code-manager",
  "version": "1.0.2",
  "description": "Simple multi-session workflow manager for Claude Code",
  "main": "server.js",
  "bin": {
    "claude-code-manager": "server.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js --dev",
    "postinstall": "node scripts/fix-pty-permissions.js",
    "prepublishOnly": "npm audit --audit-level=high",
    "release": "npm publish --access public"
  },
  "files": [
    "server.js",
    "lib/",
    "public/",
    "scripts/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "terminal",
    "session-manager",
    "cli",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwertykeith/claude-code-manager.git"
  },
  "bugs": {
    "url": "https://github.com/qwertykeith/claude-code-manager/issues"
  },
  "homepage": "https://github.com/qwertykeith/claude-code-manager#readme",
  "author": "qwertykeith",
  "dependencies": {
    "chokidar": "^3.6.0",
    "node-pty": "^1.0.0",
    "open": "^10.1.0",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}